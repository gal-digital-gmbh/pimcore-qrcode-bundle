{
  "name": "gal-digital-gmbh/pimcore-qrcode-bundle",
  "version": "2.0.0",
  "type": "pimcore-bundle",
  "description": "Pimcore QR-Code bundle",
  "keywords": [
    "pimcore",
    "qrcode",
    "bundle"
  ],
  "license": "GPL-3.0-or-later",
  "authors": [
    {
      "name": "Christian Flack",
      "email": "christian.flack@gal-digital.de",
      "homepage": "https://www.gal-digital.de",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.1",
    "endroid/qr-code": "^4.8",
    "pimcore/admin-ui-classic-bundle": "^1.0",
    "pimcore/pimcore": "^11.0.7"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.23",
    "phpstan/phpstan": "^1.10.29"
  },
  "autoload": {
    "psr-4": {
      "GalDigitalGmbh\\PimcoreQrcodeBundle\\": "src"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-main": "2.0-dev"
    },
    "pimcore": {
      "bundles": [
        "GalDigitalGmbh\\PimcoreQrcodeBundle\\PimcoreQrcodeBundle"
      ]
    }
  }
}
